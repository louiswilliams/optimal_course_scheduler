<h5><%= link_to "Back to sections",course_path(@course.id) %></h5>
<div class="row">
  <div class="span">
    <h1><%= link_to "#{@course.college} #{@course.name}",course_path(@course.id) %></h1>
  </div>
  <div class="span" style="padding-top: 10px;"><p>
    <strong><%= @course.title %></strong><br>
    <span class="text-success"><%= @course.credits %> Credit hours</span>
  </p>
  </div>
</div>

<div class="well">
  <h3>Section <%= @section.name %></h3>
  <% @meetings.each do |meeting| %>
    <% @meeting = meeting %>
    <%= render partial: "meeting/show", object: @meeting %>
  <% end %>
</div>
